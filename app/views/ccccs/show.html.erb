<div class="page-header">
  <%= link_to ccccs_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cccc_path(@cccc), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show cccc</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @cccc.nombre %></dd>

  <dt>Algo:</dt>
  <dd><%= @cccc.algo %></dd>

  <dt>Hm:</dt>
  <dd><%= @cccc.hm %></dd>

  <dt>Nacimiento:</dt>
  <dd><%= @cccc.nacimiento %></dd>

  <dt>Edad:</dt>
  <dd><%= @cccc.edad %></dd>

</dl>
