<div class="page-header">
  <%= link_to responsables_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_responsable_path(@responsable), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show responsable</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @responsable.nombre %></dd>

  <dt>Email:</dt>
  <dd><%= @responsable.email %></dd>

  <dt>Fono:</dt>
  <dd><%= @responsable.fono %></dd>

  <dt>Lugar:</dt>
  <dd><%= @responsable.lugar.nombre %></dd>

</dl>
