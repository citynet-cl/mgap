<div class="page-header">
  <%= link_to modulos_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_modulo_path(@modulo), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show modulo</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @modulo.nombre %></dd>

  <dt>Descripcion:</dt>
  <dd><%= @modulo.descripcion %></dd>


</dl>
